{
    "name": "domatskiy/pickpoint",
    "type": "library",
    "description": "PickPoint API",
    "keywords": ["PickPoint", "delivery", "postomat"],
    "homepage": "https://github.com/domatskiy",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeniy Domatskiy",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
            "type":"git",
            "url":"https://github.com/domatskiy/pickpoint"
        },
        {
            "type":"package",
            "package":{
                "name":"domatskiy/pickpoint",
                "version":"0.4.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/domatskiy/pickpoint",
                    "reference":"master"
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Domatskiy\\": "src/"
            }
        },
    "autoload-dev": {
        "psr-4": {
            "Domatskiy\\Tests\\": "tests/"
            }
        },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2"
        },
    "require-dev": {
        "phpunit/phpunit": "^5.4.0",
        "piwik/ini": "^1.0"
        }
}
